<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main_menu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="travelpals.travelpals.MainMenuActivity">

    <ImageButton
        android:layout_width="130dp"
        android:layout_height="150dp"
        android:scaleType="fitXY"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="30dp"
        app:srcCompat="@drawable/passportvector"
        android:id="@+id/ibMyPlanImageButton" />

    <ImageButton
        android:layout_width="130dp"
        android:layout_height="150dp"
        android:scaleType="fitXY"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="170dp"
        app:srcCompat="@drawable/searchvector"
        android:id="@+id/ibSearchImageButton" />

    <ImageButton
        android:layout_width="130dp"
        android:layout_height="150dp"
        android:scaleType="fitXY"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="30dp"
        app:srcCompat="@drawable/messagevector"
        android:id="@+id/ibMessageImageButton" />

    <ImageButton
        android:layout_width="130dp"
        android:layout_height="150dp"
        android:scaleType="fitXY"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="170dp"
        app:srcCompat="@drawable/userprofilevector"
        android:id="@+id/ibUserImageButton"
        />

    <TextView
        android:text="My Plan"
        android:layout_width="120dp"
        android:layout_height="20dp"
        android:background="#000"
        android:textColor="#fff"
        android:textAlignment="center"
        android:id="@+id/tvMyPlan"
        android:layout_alignBottom="@+id/ibMyPlanImageButton"
        android:layout_alignEnd="@+id/ibMyPlanImageButton"
        android:layout_alignStart="@+id/ibMyPlanImageButton" />




</RelativeLayout>
